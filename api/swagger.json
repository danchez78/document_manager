{
    "swagger": "2.0",
    "info": {
        "description": "Service for managing documents for users",
        "title": "Document Manager",
        "contact": {},
        "version": "1.0.0"
    },
    "host": "127.0.0.1:50000",
    "basePath": "/api",
    "paths": {
        "/auth": {
            "post": {
                "description": "Authenticates user by login and password and returns token",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "users"
                ],
                "summary": "Auth user",
                "parameters": [
                    {
                        "description": "Login and password",
                        "name": "auth_user_data",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/controllers.AuthUserController"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/views.Response-views_AuthUserResponse"
                        }
                    }
                }
            }
        },
        "/auth/{token}": {
            "delete": {
                "description": "Deauthenticates user by token",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "users"
                ],
                "summary": "Deauth user",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/views.Response-views_DeauthUserResponse"
                        }
                    }
                }
            }
        },
        "/docs": {
            "get": {
                "description": "Returns documents information",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "docs"
                ],
                "summary": "Get docs info",
                "parameters": [
                    {
                        "type": "string",
                        "description": "User token",
                        "name": "token",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "User login",
                        "name": "login",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "Count of documents to return. Default: 10",
                        "name": "limit",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Name of document",
                        "name": "name",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Mime of document",
                        "name": "mime",
                        "in": "query"
                    },
                    {
                        "type": "boolean",
                        "description": "Is file",
                        "name": "file",
                        "in": "query"
                    },
                    {
                        "type": "boolean",
                        "description": "Is public",
                        "name": "public",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Creation time of document",
                        "name": "created",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/views.Response-views_GetDocsInfoResponse"
                        }
                    }
                }
            },
            "post": {
                "description": "Uploads document",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "docs"
                ],
                "summary": "Upload doc",
                "parameters": [
                    {
                        "type": "file",
                        "description": "File of document",
                        "name": "file",
                        "in": "formData",
                        "required": true
                    },
                    {
                        "type": "file",
                        "description": "Meta data of document",
                        "name": "meta",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/views.Response-views_UploadDocResponse"
                        }
                    }
                }
            }
        },
        "/docs/{id}": {
            "get": {
                "description": "Returns document",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "docs"
                ],
                "summary": "Get doc",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Token of user",
                        "name": "token",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "ID of document",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "file"
                        }
                    }
                }
            },
            "delete": {
                "description": "Deletes document",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "docs"
                ],
                "summary": "Delete doc",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Token of user",
                        "name": "token",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "ID of document",
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "file"
                        }
                    }
                }
            }
        },
        "/register": {
            "post": {
                "description": "Registers user",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "users"
                ],
                "summary": "Register user",
                "parameters": [
                    {
                        "description": "Admin token and users login and password to register it with",
                        "name": "register_user_data",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/controllers.RegisterUserController"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/views.Response-views_RegisterUserResponse"
                        }
                    }
                }
            }
        }
    },
